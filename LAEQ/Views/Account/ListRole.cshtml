
@{
    ViewBag.Title = "ListRole";
}


<br />
<div class="form-horizontal">
    <div class="panel-group ">
        <div class="panel panel-default">
            <div class="panel-body">

                <h4 class="marginleft">Roles :</h4>
                <br />
                <div class="container">

                    <div class="form-horizontal">
                        <table id="machine" class="table table-bordered">
                            <thead>
                                <tr class="background-docker">
                                    <th>
                                        Role Name
                                    </th>
                                    
                                  
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var item in Model)
                                {
                                    <tr>
                                        <td>@item.Name</td>
                                       
                                    </tr>
                                }

                            </tbody>
                        </table>


                    </div>
                </div>

            </div>

        </div>
    </div>
</div>
